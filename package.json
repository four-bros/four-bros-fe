        "dev": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest",
            "eslint:recommended"
        ],
        "rules": {
            "quotes": [
                "error",
                "single"
            ],
            "no-unused-vars": [
